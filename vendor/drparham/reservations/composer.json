{
    "name": "drparham/reservations",
    "description": "A Simple Reservation Package that blocks dates out and creates JSON object to incorporate with JavaScript Calendar Systems such as fullCalendar",
    "keywords": ["Laravel", "Reservations"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Parham",
            "email": "dr.parham@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "Drparham\\Reservations\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
